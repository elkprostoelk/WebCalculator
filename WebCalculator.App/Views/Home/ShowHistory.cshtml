@model ICollection<Calculation>
@{
    ViewData["Title"] = "История результатов";
}
<div class="text-left">
    <h1>История результатов вычислений</h1>
    @if (Model.Count > 0)
    {
        <table border="1" cellpadding="1">
            <tr>
                <th>Выражение:</th>
                <th>Результат:</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Expression</td>
                    <td>@item.Result</td>
                </tr>
            }
        </table>
    }
    else
    {
        <h3>История пуста!</h3>
    }
</div>